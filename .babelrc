{
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        "transform-object-assign"
      ]
    }
  },
  "presets": ["es2015", "react", "stage-2"]
}
