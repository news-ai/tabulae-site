  "scripts": {
    "lint": "eslint .",
    "test": "node --harmony_proxies node_modules/.bin/jest",
    "test:watch": "node --harmony_proxies node_modules/.bin/jest --watch",
    "test:coverage": "node --harmony_proxies node_modules/.bin/jest --coverage",
    "start": "cross-env NODE_ENV=development node server.dev.js",
    "build": "npm run test && cross-env NODE_ENV=production webpack --config webpack.prod.config.js --progress --colors",
    "serve": "cross-env NODE_ENV=production node server.prod.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^components(.*)$": "<rootDir>/js/components$1",
      "^reducers(.*)$": "<rootDir>/js/reducers$1",
      "^constants(.*)$": "<rootDir>/js/constants$1",
      "^actions(.*)$": "<rootDir>/js/actions$1",
      "^utils(.*)$": "<rootDir>/js/utils$1"
    }
  }
